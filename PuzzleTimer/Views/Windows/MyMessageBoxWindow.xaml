<Window x:Class="PuzzleTimer.Views.Windows.MyMessageBoxWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:PuzzleTimer.Views"
         xmlns:m="clr-namespace:PuzzleTimer.Models"
        xmlns:local="clr-namespace:PuzzleTimer.Views.Windows"
        mc:Ignorable="d"
        m:WindowCloser.EnableWindowClosing="True"
        Title="{Binding TitleText, UpdateSourceTrigger=PropertyChanged}" Height="200" Width="400" ResizeMode="NoResize"  WindowStartupLocation="CenterScreen">
    <Grid>
        <views:MyMessageBoxView/>
    </Grid>
</Window>
